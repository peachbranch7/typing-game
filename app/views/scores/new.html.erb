<%= render "shared/header"%>

<body>
  <div class="typing-word-content">
    <div class="typing-title">下にタイピングする文字が出ます</div>
    <div id="typing-words"></div>
  </div>
  <div class="count-down-timer">
    <p>
      <span id="sec">0</span> 秒
    </p>
    <p>
      <input type="button" id="start" value="START">
      <input type="button" id="stop" value="STOP" disabled>
    </p>
  </div>
  <div>
    ここまで<span id="count-number">0</span>問解けました。
    <%= form_with model: @score, id:"form" do |s| %>
      <%= s.text_field :user_id, value: current_user.id, type: "hidden" %>
      <%= s.text_field :point, placeholder: "解けた問題数を入力してください。"%>
      <%= s.submit "送信", id: "end_btn" %>
    <% end %>
  </div>



  
  <%= include_gon %>
</body>

<%#= render "shared/footer"%>